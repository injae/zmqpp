# Cppkg Base Dependency Downloader
cmake_minimum_required(VERSION 3.6)
project(ZeroMQ-4.3.3-install)

set(CPPM_VERSION 0.0.12)
include(${CMAKE_CURRENT_SOURCE_DIR}/cmake/cppm_loader.cmake)
download_package(ZeroMQ 4.3.3  URL https://github.com/zeromq/libzmq/archive/v4.3.3.tar.gz  TYPE lib CMAKE_ARGS ${CMAKE_ARGS} )

